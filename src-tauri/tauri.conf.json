{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeBuildCommand": "",
    "beforeDevCommand": "",
    "devPath": "../dist",
    "distDir": "https://member.mymember.ltd"
  },
  "package": {
    "productName": "我的会员管家",
    "version": "1.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": true
    },
    "bundle": {
      "active": true,
      "category": "Business",
      "copyright": "xianzhuimu",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/256x256.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.yoonzm.member",
      "longDescription": "简单、便捷、免费的会员管理系统。适用于理发，洗车，美容等各种行业。为商家老板提供多店铺、店员、会员及其消费项目管理等多种服务。",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "免费的会员管理软件",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": "",
        "wix": {
          "language": "zh-CN"
        }
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 800,
        "resizable": false,
        "title": "我的会员管家",
        "width": 400,
        "alwaysOnTop": true
      }
    ]
  }
}
